{
  "name": "shadcn-react-table",
  "version": "0.0.1",
  "license": "MIT",
  "description": "A Tailwind/shadcn-based implementation of TanStack React Table v8 components.",
  "author": "anindosarker",
  "sideEffects": false,
  "main": "dist/index.js",
  "keywords": [
    "react-table",
    "material-ui",
    "material-table",
    "tanstack table",
    "data table",
    "shadcn"
  ],
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "pnpm lib:build",
    "lib:build": "pnpm build-lib",
    "lib:dev": "pnpm build-lib --watch",
    "build-lib": "rm -rf dist && rollup -c rollup.config.mjs",
    "dev": "pnpm lib:dev",
    "lint": "eslint . || true"
  },
  "peerDependencies": {
    "@tanstack/react-table": ">=8",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.468.0",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.8.1",
    "typescript": "5.7.2",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2"
  }
}
